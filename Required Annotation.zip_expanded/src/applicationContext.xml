<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<bean class="com.spring.Hall" id="hall" autowire="byType">
		<property name="width" value="12"></property>
		<property name="length" value="10"></property>
	</bean>
	
	<bean class="com.spring.Owner" id="Owner">
		<property name="name" value="John"></property>
		<property name="password" value="johnnyyy"></property>
	</bean>
	
</beans>
